<div class="container">
<h4>Registrovat se mohou pouze členové KCHCS</h4>
<%= link_to "Seznam členů", members_path, method: :get, class: "btn btn-success" %>
</div>

<%= render partial: "shared/error", locals: { obj: @owner } %>    
 
<div class="container">
        <%= form_with(model: @owner, local: true) do |f| %>
          <div class="mb-3">
            <%= f.label :first_name, "Křestní jméno", class: "form-label" %>
            <%= f.text_field :first_name, class: "form-control", placeholder: "First name" , value: params[:first_name]%>
          </div>
          <div class="mb-3">
            <%= f.label :last_name, "Příjmení", class: "form-label" %>
            <%= f.text_field :last_name, class: "form-control", placeholder: "Last name", value: params[:last_name] %>
          </div>
          <div class="mb-3">
            <%= f.label :email,"Email", class: "form-label" %>
            <%= f.text_field :email, class: "form-control", placeholder: "Email", value: params[:email]%>
          </div>
          <div class="mb-3">
            <%= f.label :password,"Heslo", class: "form-label" %>
            <%= f.password_field :password, class: "form-control", placeholder: "Password" %>
          </div>
          <div class="mb-3">
            <%= f.submit "Registrovat", class: "btn btn-warning" %>
          </div>
        <% end %>
</div>
    
    
    
    
    
    
    
    
    
    
    
    