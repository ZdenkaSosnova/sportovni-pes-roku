<%= render partial: "shared/error", locals: { obj: @owner } %>    
    
    <div class="container">
        <%= form_with(model: @owner, local: true) do |f| %>
          <div class="mb-3">
            <%= f.label :first_name, class: "form-label" %>
            <%= f.text_field :first_name, class: "form-control", placeholder: "First name" , value: params[:first_name]%>
          </div>
          <div class="mb-3">
            <%= f.label :last_name, class: "form-label" %>
            <%= f.text_field :last_name, class: "form-control", placeholder: "Last name", value: params[:last_name] %>
          </div>
          <div class="mb-3">
            <%= f.label :email, class: "form-label" %>
            <%= f.text_field :email, class: "form-control", placeholder: "Email", value: params[:email]%>
          </div>
          <div class="mb-3">
            <%= f.label :password, class: "form-label" %>
            <%= f.text_field :password, class: "form-control", placeholder: "Password" %>
          </div>
          <div class="mb-3">
            <%= f.submit "Sign up", class: "btn btn-primary" %>
          </div>
        <% end %>
        </div>
    
    <div class="container">
        <%= link_to "Back to the home page", root_path %>
    </div> 
    
    
    
    
    
    
    
    
    
    
    
    
    